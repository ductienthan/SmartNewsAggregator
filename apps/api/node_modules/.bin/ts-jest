#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ducthan/nestjs/smart-news-aggregator/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_bc19dbb917d5462d708cf3ba15190b2a/node_modules/ts-jest/node_modules:/Users/ducthan/nestjs/smart-news-aggregator/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_bc19dbb917d5462d708cf3ba15190b2a/node_modules:/Users/ducthan/nestjs/smart-news-aggregator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ducthan/nestjs/smart-news-aggregator/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_bc19dbb917d5462d708cf3ba15190b2a/node_modules/ts-jest/node_modules:/Users/ducthan/nestjs/smart-news-aggregator/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_bc19dbb917d5462d708cf3ba15190b2a/node_modules:/Users/ducthan/nestjs/smart-news-aggregator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_bc19dbb917d5462d708cf3ba15190b2a/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.0.5_@jest+types@30.0.5_babel-jest@_bc19dbb917d5462d708cf3ba15190b2a/node_modules/ts-jest/cli.js" "$@"
fi
